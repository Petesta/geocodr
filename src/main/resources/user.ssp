<html>
<head>
  <meta name="viewport" content="width=device-width" />
  <link href="http://fonts.googleapis.com/css?family=Open+Sans:300" rel="stylesheet" type="text/css" />
  <link rel="stylesheet" href="/assets/fonts/font-awesome/css/font-awesome.css">
  <link href="/assets/css/application.css" media="screen" type="text/css" rel="stylesheet" />
</head>
<body class="noise">

  <%@ var username: String %>
  <!--<h1>Hello ${username}</h1>-->

  <div class="users-container">

    <section class="panel panel-photos">
      <i class="icon panel-icon icon-group"></i>


      <div class="user-photos-container">
        <div class="user">
          <img class="user-photo" src="" alt="" />
          <p>You</p>
        </div>

        <div class="link"></div>


        <div class="user">
          <img class="user-photo" src="" alt="" />
          <p>Tom Smith</p>
        </div>
      </div>

    </section>



    <section class="panel panel-langs">
      <i class="icon panel-icon icon-code"></i>

      <p>You and Tom both write Ruby.</p>

      <div class="charts-container">
        <div class="chart"></div>
        <div class="chart"></div>
      </div>

    </section>



    <section class="panel panel-stars">
      <i class="icon panel-icon icon-star-empty"></i>

      <p>You and Tom have 13 starred repositories in common.</p>

    </section>



    <section class="panel panel-follow">
      <h2>Follow Tom</h2>

      <a class="btn btn-large btn-blue btn-follow-cta" href="#">Follow Tom</a>
    </section>

  </div><!-- .users-container -->


</body>
</html>
